<div id="<%= dom_id contact %>">
  <p>
    <strong>Full name:</strong>
    <%= contact.full_name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= contact.company %>
  </p>

  <p>
    <strong>Photo path:</strong>
    <%= contact.photo_path %>
  </p>

  <p>
    <strong>Phone no:</strong>
    <%= contact.phone_no %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

</div>
